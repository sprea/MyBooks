<!DOCTYPE html>
<html lang="it">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Libreria</title>
    <%- include('./partials/navbar'); %>
</head>
<body>

    <div id="libreria">

        <% rows.forEach(libro => { %>
            <h3><%= libro.Titolo %></h3>
            <h3><%= libro.Isbn %></h3>
            <h3><%= libro.Autore %></h3>
            <h3><%= libro.Pagine %></h3>
            <h3><%= libro.PagineLette %></h3>
            <h3><%= libro.Completato %></h3>
            <h3><%= libro.Impressioni %></h3>
            <h3><%= libro.Valutazione %></h3>
        <% }) %>
    </div>

</body>
</html>